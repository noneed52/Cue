<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.cue">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LandingPageActivity" />
        <activity android:name=".ManageAccountActivity" />
        <activity android:name=".MyStatsActivity" />
        <activity android:name=".TodayCueActivity" />
        <activity android:name=".RescheduleActivity" />
        <activity android:name=".CueHistoryActivity"></activity>
    </application>

</manifest>